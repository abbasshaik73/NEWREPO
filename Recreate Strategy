---
apiVersion: apps/v1
kind: Deployment
metadata:
  name: abbas
spec:
  replicas: 2
  selector:
    matchLabels:
      app: swiggy
  template:
    metadata:
      name: new
      labels:
        app: swiggy
    spec:
      containers:
        - name: mycont
          image: shaikmustafa/paytm:movie
          ports:
            - containerPort: 80


---
apiVersion: v1
kind: Service
metadata:
  name: mysvc
spec:
  type: LoadBalancer
  selector:
    app: swiggy
  ports:
    - port: 80
      targetPort: 80
